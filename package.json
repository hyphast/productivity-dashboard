{
  "name": "productivity-dashboard",
  "version": "0.1.0",
  "private": true,
  "author": "Oleg Shcherbakov <oshcherbakovv@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "preview": "serve --listen 3001 --no-clipboard --single dist",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "stylelint:check": "stylelint \"**/*.scss\"",
    "code:check": "prettier --check \"src/**/*.{ts,tsx,js,css,scss,html}\"",
    "code:format": "prettier --write \"src/**/*.{ts,tsx,js,css,scss,html}\"",
    "code:format:specific-file": "prettier --write",
    "husky:postinstall": "husky install",
    "update:check": "yarn outdated",
    "update:interactive": "yarn upgrade-interactive",
    "update:all": "yarn yarn-upgrade-all"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "boring-avatars": "^1.10.1",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "firebase": "^10.1.0",
    "immer": "^10.0.2",
    "postcss-responsive-type": "^1.0.0",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.45.4",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.15.0",
    "react-transition-group": "^4.4.5",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@pmmmwh/react-refresh-webpack-plugin": "github:pmmmwh/react-refresh-webpack-plugin",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv-webpack": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.3.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^3.0.1",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.1",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "serve": "^14.2.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-prettier": "^4.0.2",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "yarn-upgrade-all": "^0.7.2"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "./src/**/*.{scss}": [
      "prettier --ignore-path .gitignore --write"
    ]
  }
}
